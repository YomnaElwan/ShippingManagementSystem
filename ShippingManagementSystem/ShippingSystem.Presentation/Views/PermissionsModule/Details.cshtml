@using ShippingSystem.Domain.Entities
@model PermissionsModule
@{
    ViewData["Title"] = "Details";
}

<div class="table-responsive mt-3">
    <table class="table table-hover table-bordered align-middle text-center bg-light bg-opacity-75 shadow-sm rounded">
        <thead class="table-primary">
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Create At</td>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.Id</td>
                <td>@Model.Name</td>
                <td>@Model.CreateAt</td>
            </tr>
        </tbody>
    </table>
    <a asp-action="Index" asp-controller="PermissionsModule" class="btn btn-dark mt-3"><i class="bi bi-arrow-left"></i> Back To List</a>
</div>
